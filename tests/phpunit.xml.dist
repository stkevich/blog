<phpunit bootstrap="../vendor/autoload.php">
	<testsuites>
		<testsuite name="Unit">
			<directory suffix="Test.php">./unit</directory>
		</testsuite>
	</testsuites>
	<filter>
		<whitelist addUncoveredFilesFromWhitelist="true">
			<directory suffix=".php">../src</directory>
		</whitelist>
	</filter>
</phpunit>
